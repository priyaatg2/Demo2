pipeline {
    agent any

    stages {
        stage('build') {
            steps {
                bat 'javac jenkinjava.java'
                echo 'building'
            }
        }
        stage('test') {
            steps {
                echo 'testing'
            }
        }
        stage('deploy') {
            steps {
               
                bat 'java jenkinjava'
                echo 'deploying'
            }
        }
    }
}
